{
  "name": "gabri.me",
  "version": "4.0.0",
  "description": "Some guy on the web",
  "scripts": {
    "clean": "rm -rf ./__build__",
    "generateData": "node ./bin/generateData.js",
    "generateFeed": "node ./bin/generateFeed.js",
    "assets": "cp -R ./public/* _redirects __build__/",
    "build": "yarn run clean && yarn run generateData && webpack && yarn run generateFeed && npm run assets",
    "foo": "webpack-dev-server --progress --colors --hot --inline --history-api-fallback",
    "dev": "yarn run clean && node server/dev.js",
    "start": "yarn run build && node server/prod.js"
  },
  "private": true,
  "author": "Ahmed El Gabri <ahmed@gabri.me>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-env": "1.1.8",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "compression": "1.6.2",
    "css-loader": "0.26.1",
    "cxs": "3.0.3",
    "date-fns": "1.25.0",
    "express": "4.14.0",
    "file-loader": "0.9.0",
    "front-matter": "2.1.1",
    "highlight.js": "9.9.0",
    "html-webpack-plugin": "2.26.0",
    "markdown-it": "8.2.2",
    "ramda": "0.23.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "3.3.0",
    "react-router": "4.0.0-alpha.6",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-hot-middleware": "2.15.0"
  }
}
