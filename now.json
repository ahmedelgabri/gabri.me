{
	"version": 2,
	"regions": ["all"],
	"alias": ["gabri.me", "www.gabri.me"],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1.js"
		},
		{
			"src": "/(blog|work)",
			"status": 301,
			"headers": {"Location": "https://gabri.me"}
		},
		{
			"src": "/(blog|work)/",
			"status": 301,
			"headers": {"Location": "https://gabri.me"}
		},
		{
			"src": "/feed",
			"status": 301,
			"headers": {"Location": "https://gabri.me/feed.xml"}
		},
		{
			"src": "/feed/",
			"status": 301,
			"headers": {"Location": "https://gabri.me/feed.xml"}
		}
	]
}
