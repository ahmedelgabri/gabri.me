{
  "defaultSeverity": "error",
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "interface-name": false,
    "member-access": [false],
    "ordered-imports": [true],
    "no-console": [true],
    "no-var-requires": true
  }
}
